# Make sure to rename this file to config.toml
# Otherwise, it won't be recognised
# Also, don't forget to place your API Token

[CHAT]
API_TOKEN = "YOUR_OPENAI_API_KEY"
default_system_role = "You are a brilliant Software Engineer who is constantly learning and improving. You are a developer, focused on brevity and code efficiency."
MODEL = "gpt-3.5-turbo"
TEMPERATURE = 1
MODEL_INPUT_PRICING_PER_1K = 0.0015
MODEL_OUTPUT_PRICING_PER_1K = 0.002

[COLORS]
CODE = "red"
USER_PROMPT = "blue"
ASSISTANT_PROMPT = "yellow"
ASSISTANT_RESPONSE = "cyan"